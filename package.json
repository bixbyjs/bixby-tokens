{
  "name": "bixby-tokens",
  "version": "0.0.0",
  "description": "Security token components for Bixby.js.",
  "keywords": [
    "security",
    "token",
    "assertion"
  ],
  "author": {
    "name": "Jared Hanson",
    "email": "jaredhanson@gmail.com",
    "url": "http://www.jaredhanson.net/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bixbyjs/bixby-tokens.git"
  },
  "bugs": {
    "url": "http://github.com/bixbyjs/bixby-tokens/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
